<Window x:Class="CanMakeTree.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CanMakeTree"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>        
        <TextBox x:Name="txtSmiles" HorizontalAlignment="Left" Height="30" Margin="75,20,0,0" Text="请输入Smiles" TextWrapping="Wrap" VerticalAlignment="Top" Width="370"/>
        <Label x:Name="label" Content="Smiles" HorizontalAlignment="Left" Height="30" Margin="20,20,0,0" VerticalAlignment="Top" Width="50"/>
        <Label x:Name="moId" Content="Smiles" HorizontalAlignment="Left" Height="30" Margin="450,20,0,0" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="txtMoleculeID" HorizontalAlignment="Left" Height="30" Margin="500,20,0,0" Text="请输入Smiles" TextWrapping="Wrap" VerticalAlignment="Top" Width="80"/>
        <Button x:Name="button" Content="确定" HorizontalAlignment="Left" Height="30" Margin="670,20,0,0" VerticalAlignment="Top" Width="80" Click="button_Click"/>
    </Grid>
</Window>
